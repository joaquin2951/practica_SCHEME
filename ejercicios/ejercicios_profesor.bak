(define longitud
(lambda(lista)
(if(null? lista)
0
(+ 1 (longitud (cdr lista)))
)
)
)

;(reversa '(1 2 3))
; (reversa '(1 '(2 3) 3))

(define reversaProfundidad
(lambda(lst)
(if (null? lst) lst
(append (reversaProfundidad (cdr lst))
(list (if (list? (car lst)) (reversaProfundidad (car lst)) (car lst)))
)
)
)
)



